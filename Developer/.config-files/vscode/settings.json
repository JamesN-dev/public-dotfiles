{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": true
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "animations.Install-Method": "Apc Customize UI++",
  "apc.imports": [
    "file://${userHome}/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js"
  ],
  "cmake.options.statusBarVisibility": "icon",
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "cmake.showOptionsMovedNotification": false,
  "editor.accessibilitySupport": "off",
  "editor.formatOnSave": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": false,
    "python": true,
    "scminput": false
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "makefile.configureOnOpen": true,
  "mcp": {
    "servers": {
      "desktop-commander": {
        "args": [
          "-y",
          "@smithery/cli@latest",
          "run",
          "@wonderwhy-er/desktop-commander",
          "--key",
          "d580f0d8-f921-4cd3-aabd-0978c2be3f6c"
        ],
        "command": "npx"
      },
      "toolbox": {
        "args": [
          "-y",
          "@smithery/cli@latest",
          "run",
          "@smithery/toolbox",
          "--key",
          "d580f0d8-f921-4cd3-aabd-0978c2be3f6c",
          "--profile",
          "relieved-donkey-SbCHuO"
        ],
        "command": "npx"
      }
    }
  },
  "python.formatting.provider": "none",
  "redhat.telemetry.enabled": false,
  "roo-cline.allowedCommands": [
    "npm test",
    "npm install",
    "tsc",
    "git log",
    "git diff",
    "git show"
  ],
  "ruff.lineLength": 88,
  "settingsSync.ignoredExtensions": [
    "batisteo.vscode-django",
    "dbaeumer.vscode-eslint"
  ],
  "supermaven.enable": {
    "*": false
  },
  "supermaven.logFilePath": "/Volumes/HDD-Storage/development/SuperMaven/logs",
  "svelte.enable-ts-plugin": true,
  "terminal.external.osxExec": "wezterm.app",
  "window.zoomLevel": 0.25,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontSize": 12,
  "theme-to-zed.theme_importer": "/usr/local/bin/theme_importer",
  "window.newWindowProfile": "jamesn-dev",
  "zig.zls.enabled": "on"
}