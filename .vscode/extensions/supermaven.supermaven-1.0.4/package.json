{
	"name": "supermaven",
	"displayName": "Supermaven",
	"publisher": "Supermaven",
	"description": "The fastest copilot.",
	"version": "1.0.4",
	"icon": "assets/logo-square.png",
	"engines": {
		"vscode": "^1.68.0"
	},
	"bugs": {
		"email": "support@supermaven.com"
	},
	"categories": [
		"Machine Learning",
		"Programming Languages",
		"Snippets"
	],
	"keywords": [
		"ai",
		"autocomplete",
		"intellicode",
		"intellisense",
		"code completion",
		"bash",
		"c",
		"cpp",
		"csharp",
		"css",
		"dart",
		"elixir",
		"erlang",
		"go",
		"java",
		"javascript",
		"json",
		"kotlin",
		"lua",
		"markdown",
		"php",
		"python",
		"ruby",
		"rust",
		"scala",
		"shell",
		"sql",
		"swift",
		"typescript",
		"yaml",
		"super",
		"maven",
		"copilot"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/main.js",
	"contributes": {
		"codeActions": [
			{
				"languages": [
					"*"
				],
				"kind": "quickfix"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "supermaven-chat",
					"title": "Supermaven",
					"icon": "assets/activity-bar-logo.svg"
				}
			]
		},
		"views": {
			"supermaven-chat": [
				{
					"type": "webview",
					"id": "supermaven-view",
					"name": "chat"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "supermaven.toggleSettings",
					"group": "navigation@1",
					"when": "view == supermaven-view"
				},
				{
					"command": "supermaven.toggleChatMenu",
					"group": "navigation@2",
					"when": "view == supermaven-view"
				},
				{
					"command": "supermaven.toggleModelSelectorMenu",
					"group": "navigation@3",
					"when": "view == supermaven-view"
				},
				{
					"command": "supermaven.newConversationTab",
					"group": "navigation@4",
					"when": "view == supermaven-view"
				}
			],
			"editor/context": [
				{
					"command": "supermaven.editWithSupermaven",
					"when": "editorHasSelection"
				},
				{
					"command": "supermaven.addToChat",
					"when": "editorHasSelection"
				}
			]
		},
		"commands": [
			{
				"command": "supermaven.toggle",
				"title": "Supermaven: Toggle"
			},
			{
				"command": "supermaven.logout",
				"title": "Supermaven: Log Out"
			},
			{
				"command": "supermaven.newConversationTab",
				"title": "New Chat",
				"category": "Supermaven Chat",
				"icon": "$(terminal-new)"
			},
			{
				"command": "supermaven.toggleModelSelectorMenu",
				"title": "Select Model",
				"category": "Supermaven Chat",
				"icon": "$(robot)"
			},
			{
				"command": "supermaven.toggleChatMenu",
				"title": "History",
				"category": "Supermaven Chat",
				"icon": "$(history)"
			},
			{
				"command": "supermaven.toggleSettings",
				"title": "Settings",
				"category": "Supermaven Chat",
				"icon": "$(gear)"
			},
			{
				"command": "supermaven.editWithSupermaven",
				"title": "Supermaven: Edit"
			},
			{
				"command": "supermaven.addToChat",
				"title": "Supermaven: Add to Chat"
			},
			{
				"command": "supermaven.fixWithSupermaven",
				"title": "Supermaven: Fix"
			}
		],
		"keybindings": [
			{
				"command": "supermaven.editWithSupermaven",
				"key": "ctrl+i",
				"mac": "cmd+i"
			},
			{
				"command": "supermaven.addToChat",
				"key": "ctrl+shift+i",
				"mac": "cmd+shift+i"
			},
			{
				"command": "supermaven.newConversationTab",
				"key": "alt+j"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Supermaven",
			"properties": {
				"supermaven.logFilePath": {
					"type": [
						"string",
						"null"
					],
					"default": null,
					"description": "Path to log file. If present, Supermaven will write logs of its internal operations to this file."
				},
				"supermaven.enable": {
					"type": "object",
					"default": {},
					"description": "This setting allows disabling Supermaven for specific languages. The key is the language ID, and the value is a boolean indicating whether Supermaven should be enabled for that language. If the value is false, Supermaven will not be enabled for that language. The key '*' can be used to disable Supermaven for all languages."
				},
				"supermaven.allowGitignore": {
					"type": "boolean",
					"default": false,
					"description": "By default, Supermaven is disabled in files that are ignored by Git due to a .gitignore file. This setting lets you enable Supermaven in such files."
				},
				"supermaven.otherWarning": {
					"type": "boolean",
					"default": true,
					"description": "Supermaven will show a warning on startup if other inline completion extensions are enabled, possibly resulting in complications. This setting lets you disable this warning."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "rm -r out && npm run esbuild-base -- --minify && stat main.chat.js",
		"esbuild-base": "esbuild ./src/main.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
		"esbuild": "npm run esbuild-base -- --sourcemap",
		"esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
		"test-compile": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/jest": "^29.5.12",
		"@types/mocha": "^10.0.1",
		"@types/node": "20.2.5",
		"@types/vscode": "^1.68.0",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"@vscode/test-electron": "^2.3.2",
		"esbuild": "^0.20.0",
		"eslint": "^8.41.0",
		"glob": "^8.1.0",
		"mocha": "^10.2.0",
		"ts-jest": "^29.1.2",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"axios": "^1.5.1",
		"semver": "^7.5.4"
	},
	"__metadata": {
		"id": "b16722b2-9d79-4b90-8f9c-d385d23eeeed",
		"publisherId": "7c26f250-119f-4477-ba2f-62ef02b802ec",
		"publisherDisplayName": "Supermaven",
		"targetPlatform": "undefined",
		"isApplicationScoped": false,
		"isPreReleaseVersion": false,
		"hasPreReleaseVersion": false,
		"installedTimestamp": 1720667893496,
		"pinned": false,
		"preRelease": false,
		"source": "gallery"
	}
}